<p style="color: green"><%= notice %></p>

<h1>Peças</h1>
<hr>
<%= link_to "Cadastrar Peça", new_part_path, class:"btn btn-success" %>
<hr>
<div id="parts">
  <% @parts.each do |part| %>
    <%= render part %>
    <p>
      <%= link_to "Show this part", part, class:"btn btn-primary" %>
    </p>
    <hr>
  <% end %>
</div>


